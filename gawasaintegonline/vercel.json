{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/(css|js|images|fonts|assets)/(.*)", "dest": "/$1/$2" },
    { "src": "/(.*).php", "dest": "/$1.php" },
    { "src": "/", "dest": "/index.php" }
  ]
}
